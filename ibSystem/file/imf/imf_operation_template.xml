<?xml version="1.0" encoding="UTF-8"?>
<IMFRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../imf_client.xsd">
	<SysInfo>
		<MessageSequenceID>@MessageSequenceID@</MessageSequenceID>
		<MessageType>Operation</MessageType>
		<ServiceType>FSS2</ServiceType>
		<OperationMode>SYS</OperationMode>
		<SendDateTime>@SendDateTime@</SendDateTime>
		<CreateDateTime>@CreateDateTime@</CreateDateTime>
		<OriginalDateTime>@OriginalDateTime@</OriginalDateTime>
		<Receiver>@Receiver@</Receiver>
		<Sender>@Sender@</Sender>
		<Owner>@Owner@</Owner>
		<Priority>4</Priority>
	</SysInfo>
	<Data>
		<PrimaryKey>
			<FlightKey>
				<FlightDirection>@FlightDirection@</FlightDirection>
				<DetailedIdentity>
				<AirlineIATACode>@AirlineIATACode@</AirlineIATACode>
				</DetailedIdentity>
			</FlightKey>
			<BasicDataKey>
				<BasicDataCategory>@BasicDataCategory@</BasicDataCategory>
			</BasicDataKey>
		</PrimaryKey>
		<FlightData>
			<General>
				<FlightServiceType>@FlightServiceType@</FlightServiceType>
			</General>
			<Airport>
			  <Terminal>
			  <FlightTerminalID>@FlightTerminalID@</FlightTerminalID>
			  </Terminal>
			</Airport>
		</FlightData>
	</Data>
	<Operation>
		<HeartBeat>
			<HeartBeatResponse>
				<ServiceStatus>@ServiceStatus@</ServiceStatus>
				<HeartBeatRequestID>@HeartBeatRequestID@</HeartBeatRequestID>
			</HeartBeatResponse>
		</HeartBeat>
		<Subscription>
			<SyncPeriodRequest>
				<SyncUpdateFromDateTime>@SyncUpdateFromDateTime@</SyncUpdateFromDateTime>
				<SyncUpdateEndDateTime>@SyncUpdateEndDateTime@</SyncUpdateEndDateTime>
			</SyncPeriodRequest>
			<FlightPeriodRequest>
				<FlightScheduleFromDateTime>@FlightScheduleFromDateTime@</FlightScheduleFromDateTime>
				<FlightScheduleEndDateTime>@FlightScheduleEndDateTime@</FlightScheduleEndDateTime>
			</FlightPeriodRequest>
			<SubscribeResponse>
				<SubscriptionStatus>@SubscriptionStatus@</SubscriptionStatus>
				<SubscriptionRequestID>@SubscriptionRequestID@</SubscriptionRequestID>
			</SubscribeResponse>
		</Subscription>
		<Unsubscription>
			<UnsubscriptionResponse>
				<UnsubscriptionRequestID>@UnsubscriptionRequestID@</UnsubscriptionRequestID>
				<UnsubscriptionStatus>@UnsubscriptionStatus@</UnsubscriptionStatus>
			</UnsubscriptionResponse>
		</Unsubscription>
		<SystemException>
			<ExceptionCode>@ExceptionCode@</ExceptionCode>
			<ExceptionMessage>@ExceptionMessage@</ExceptionMessage>
		</SystemException>
	</Operation>
</IMFRoot>